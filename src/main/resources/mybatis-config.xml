<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.skycastle.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.skycastle.group.model.vo.Groups" alias="Groups"/>
		<typeAlias type="com.kh.skycastle.group.model.vo.GroupNotice" alias="GroupNotice"/>
		<typeAlias type="com.kh.skycastle.member.model.vo.Grade" alias="Grade"/>		
		<typeAlias type="com.kh.skycastle.cs.model.vo.Notice" alias="Notice"/>
		<typeAlias type="com.kh.skycastle.cs.model.vo.Qna" alias="Qna"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/groups-mapper.xml"/>
		<mapper resource="mappers/adMember-mapper.xml"/>
		<mapper resource="mappers/adGroups-mapper.xml"/>		
		<mapper resource="mappers/cs-mapper.xml"/>
	</mappers>
	
	

</configuration>
